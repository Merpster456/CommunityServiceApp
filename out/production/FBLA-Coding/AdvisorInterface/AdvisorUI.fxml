<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.shape.Rectangle?>
<AnchorPane minHeight="768" minWidth="1366" prefHeight="768" prefWidth="1366" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AdvisorInterface.AdvisorController">
    <HBox styleClass="primaryDark" alignment="CENTER" prefHeight="125" prefWidth="1366" />
    <Label fx:id="topLabel" styleClass="topLabel" layoutX="20" layoutY="35"/>
    <Button fx:id="studentHours" layoutY="125"  prefHeight="160.75" prefWidth="225" styleClass="Columns" text="Student&#10;Hours" textAlignment="CENTER" onAction="#studentHours" onMouseEntered="#setStudentHours" onMouseExited="#refresh"/>
    <Button fx:id="chapterProgress" layoutY="285.75" prefHeight="160.75" prefWidth="225" styleClass="Columns" text="Chapter&#10;Progress" textAlignment="CENTER" onAction="#studentProgress" onMouseEntered="#setChapterProgress" onMouseExited="#refresh"/>
    <Button fx:id="manageUsers" layoutY="446.5" prefHeight="160.75" prefWidth="225" styleClass="Columns" text="Manage&#10;Users" textAlignment="CENTER" onAction="#manageUsers" onMouseEntered="#setManageUsers" onMouseExited="#refresh"/>
    <Rectangle fill="#5377a1" layoutY="607.25" height="160.75" width="225"/>
    <ImageView fx:id="imageView" layoutX="10" layoutY="627.25"/>
    <stylesheets>
        <URL value="@../Style.css" />
    </stylesheets>
</AnchorPane>
