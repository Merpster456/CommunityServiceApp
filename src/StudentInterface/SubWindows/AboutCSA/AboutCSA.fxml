<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane minHeight="768" minWidth="1366" prefHeight="768" prefWidth="1366" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="StudentInterface.SubWindows.AboutCSA.AboutCSA">
    <HBox alignment="CENTER" prefHeight="125" prefWidth="1366" styleClass="primaryDark">
        <Label contentDisplay="CENTER" styleClass="topLabel" text="About CSA" />
    </HBox>
    <Button fx:id="back" layoutX="20" layoutY="35" onAction="#back" onMouseEntered="#backChange" onMouseExited="#refresh" styleClass="back" text="Back" />
    <VBox alignment="TOP_CENTER" layoutX="300" layoutY="175" spacing="25">
        <padding>
            <Insets bottom="50" top="25"/>
        </padding>
        <Label styleClass="title" text="Community Service Awards" />
        <Label styleClass="subHeader" text="• CSA Community = 50" />
        <Label styleClass="subHeader" text="• CSA Service = 200" />
        <Label styleClass="subHeader" text="• CSA Achievement = 500" />
        <Label styleClass="subHeader" text="• Community and Service Awards Deadline is March 1st" />
        <Label styleClass="subHeader" text="• Achievement Awards Deadline is April 25th" />
        <HBox alignment="BOTTOM_CENTER">
            <Label styleClass="grayFont" text="For any questions regarding registration click " />
            <Hyperlink onAction="#link" styleClass="link" text="here">
                <padding>
                   <Insets top="10.0" />
                </padding>
            </Hyperlink>
            <Label styleClass="grayFont" text="!" />
        </HBox>
        <padding>
            <Insets top="25" />
        </padding>
    </VBox>
    <stylesheets>
        <URL value="@../../../Style.css" />
    </stylesheets>
</AnchorPane>
